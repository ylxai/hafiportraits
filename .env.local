# ===== CAMERA CONFIGURATION =====
# Camera model: NIKON_D7100, CANON_EOS, SONY_ALPHA
DSLR_CAMERA_MODEL=NIKON_D7100

# Watch folder (auto-detected if not specified)
# Windows: C:/DCIM/100NIKON
# macOS: /Volumes/NIKON D7100/DCIM/100NIKON
# Linux: /media/NIKON/DCIM/100NIKON
DSLR_WATCH_FOLDER=C:/DCIM/100NIKON

# Auto-detect camera folder
DSLR_AUTO_DETECT=true

# Connection check interval (milliseconds)
DSLR_CONNECTION_CHECK_INTERVAL=30000

# ===== STORAGE CONFIGURATION =====
# Backup folders
DSLR_BACKUP_FOLDER=./dslr-backup
DSLR_RAW_FOLDER=./dslr-backup/raw
DSLR_JPG_FOLDER=./dslr-backup/jpg

# Enable local backup
DSLR_ENABLE_BACKUP=true

# Maximum backup size (GB)
DSLR_MAX_BACKUP_SIZE_GB=50

# Cleanup old files
DSLR_CLEANUP_OLD_FILES=false
DSLR_CLEANUP_DAYS=30

# ===== API CONFIGURATION =====
# API base URL
DSLR_API_BASE_URL=https://hafiportrait.photography

# API timeouts (milliseconds)
DSLR_API_TIMEOUT=30000
DSLR_HEALTH_CHECK_TIMEOUT=3000

# Retry configuration
DSLR_API_RETRY_ATTEMPTS=3
DSLR_API_RETRY_DELAY=5000

# ===== EVENT CONFIGURATION =====
# Event ID (REQUIRED - change this!)
#DSLR_EVENT_ID=hafiportrait-production-test-1754502822

# Photographer details
DSLR_UPLOADER_NAME=Hafiportrait
DSLR_ALBUM_NAME=Official

# Auto-create event if not exists
DSLR_AUTO_CREATE_EVENT=false

# ===== FILE PROCESSING =====
# Supported file extensions (comma-separated)
DSLR_SUPPORTED_EXTENSIONS=.jpg,.jpeg,.nef,.raw

# Maximum file size (MB)
DSLR_MAX_FILE_SIZE_MB=100

# Process RAW files
DSLR_PROCESS_RAW_FILES=false

# Process video files
DSLR_PROCESS_VIDEO_FILES=false

# ===== PERFORMANCE CONFIGURATION =====
# Performance profile: DEVELOPMENT, PRODUCTION, HIGH_VOLUME, LOW_BANDWIDTH
DSLR_PERFORMANCE_PROFILE=PRODUCTION

# ===== NOTIFICATION CONFIGURATION =====
# Enable notifications
DSLR_NOTIFICATIONS_ENABLED=true

# Local logging
DSLR_LOCAL_LOGGING=true
DSLR_LOG_FILE=./dslr-notifications.log

# Milestone notifications
DSLR_MILESTONE_NOTIFICATIONS=true
DSLR_MILESTONES=10,25,50,100,250,500,1000

# Client notifications
DSLR_CLIENT_NOTIFICATIONS=false
DSLR_CLIENT_MILESTONE_THRESHOLD=50

# ===== SECURITY CONFIGURATION =====
# API key for authentication
DSLR_API_KEY=

# Enable authentication
DSLR_ENABLE_AUTH=false

# Allowed IP addresses (comma-separated)
DSLR_ALLOWED_IPS=

# Rate limiting
DSLR_RATE_LIMIT_REQUESTS=100
DSLR_RATE_LIMIT_WINDOW_MS=60000

# ===== MONITORING CONFIGURATION =====
# Enable metrics collection
DSLR_ENABLE_METRICS=false
DSLR_METRICS_INTERVAL=60000

# Logging
DSLR_LOG_LEVEL=info
DSLR_DEBUG=false

# Health check endpoint
DSLR_HEALTH_CHECK_ENDPOINT=/api/test/db

# ===== WATERMARK CONFIGURATION =====
# Enable/disable watermark processing
DSLR_WATERMARK_ENABLED=false

# Logo file path (PNG with transparency recommended)
DSLR_WATERMARK_LOGO_PATH=./assets/logo.png

# Watermark position: bottom-center, bottom-left, bottom-right, top-center, top-left, top-right
DSLR_WATERMARK_POSITION=bottom-center

# Offset from bottom edge (pixels)
DSLR_WATERMARK_OFFSET_Y=50

# Logo opacity (0.0 to 1.0)
DSLR_WATERMARK_OPACITY=0.7

# Logo size ratio relative to image width (0.1 = 10% of image width)
DSLR_WATERMARK_SIZE_RATIO=0.15

# Output quality for watermarked images (1-100)
DSLR_WATERMARK_QUALITY=95

# ===== ADVANCED FEATURES =====
# Image compression
DSLR_ENABLE_COMPRESSION=false
DSLR_COMPRESSION_QUALITY=0.9

# Metadata extraction
DSLR_ENABLE_METADATA_EXTRACTION=false

# Parallel processing
DSLR_PARALLEL_UPLOADS=3
DSLR_QUEUE_SIZE_LIMIT=100
